{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/theia/scripts/rebuild.js",
            "args": [
                "--target=electron",
                "--modules=node-pty"
            ],
            "outFiles": [
                "${workspaceRoot}/node_modules/electron-rebuild/**/*.js",
                "${workspaceRoot}/theia/scripts/rebuild.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Electron",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "program": "${workspaceRoot}/turn-app-electron/src-gen/frontend/electron-main.js",
            "protocol": "inspector",
            "args": [
                "--loglevel=debug",
                "--hostname=localhost",
                "--no-cluster",
                "--app-project-path=${workspaceRoot}",
                "--no-app-auto-install"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/turn-app-electron/src-gen/frontend/electron-main.js",
                "${workspaceRoot}/turn-app-electron/src-gen/backend/main.js",
                "${workspaceRoot}/turn-app-electron/lib/**/*.js"
            ],
            "smartStep": true,
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std"
        }
    ]
}